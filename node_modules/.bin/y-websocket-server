#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/y-websocket@1.5.4_yjs@13.6.24/node_modules/y-websocket/bin/node_modules:/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/y-websocket@1.5.4_yjs@13.6.24/node_modules/y-websocket/node_modules:/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/y-websocket@1.5.4_yjs@13.6.24/node_modules:/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/y-websocket@1.5.4_yjs@13.6.24/node_modules/y-websocket/bin/node_modules:/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/y-websocket@1.5.4_yjs@13.6.24/node_modules/y-websocket/node_modules:/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/y-websocket@1.5.4_yjs@13.6.24/node_modules:/mnt/hp_nvme_1tb/Projects/GitHub/stormic-lexical-editor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../y-websocket/bin/server.js" "$@"
else
  exec node  "$basedir/../y-websocket/bin/server.js" "$@"
fi
