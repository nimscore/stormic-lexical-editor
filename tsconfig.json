{
	"compilerOptions": {
		"target": "ES2022",
		"module": "esnext",
		"moduleResolution": "bundler",
		"jsx": "react",
		"declaration": true,
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		// "moduleResolution": "node",
		"baseUrl": ".",
		"types": ["react", "react-dom"]
	},
	"include": ["src"]
}
